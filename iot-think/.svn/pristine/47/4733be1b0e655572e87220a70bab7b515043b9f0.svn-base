package com.rz.iot.think.mqtt.model;

import lombok.Data;

/**
 * Author by xuxiake, Date on 2019/4/12.
 * PS: Not easy to write code, please indicate.
 * Description：集中器输入
 */
@Data
public class ConcentratorLineIn {

    // 进线名称
    private String name;
    // 进线类型 1：A相；2：B相；3：C相；4：合相
    private Integer type;
    // 电压
    private Integer voltage;
    // 电流
    private Integer electricCurrent;
    // 功率因素
    private Integer powerFactor;
    // 视在功率
    private Integer apparentPower;
    // 有功功率
    private Integer activePower;
    // 无功功率
    private Integer reactivePower;
    // 电缆温度
    private Integer cableTemperature;
}
