  <!--策略详情-->
  <select id="detail" resultMap="ResultMapDetail">
    SELECT
      play_strategy.id,
      play_strategy.name,
      play_strategy.start_date,
      play_strategy.end_date,
      play_strategy.start_time,
      play_strategy.end_time,
      play_strategy.weekfilter
    FROM
