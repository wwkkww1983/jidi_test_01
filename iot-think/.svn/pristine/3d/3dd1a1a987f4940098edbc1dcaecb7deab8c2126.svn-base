  <!--查找未与路灯绑定的wifi-->
  <select id="findNoBinding" resultMap="ResultMapNoBinding">
    SELECT
      t1.*
    FROM
      (
        SELECT wifi.*
        FROM
          iot_wifi wifi
        WHERE wifi.status != ${@com.rz.iot.think.utils.RzIotDBConstParam@DEVICE_STATUS_OF_DELETE}
      ) t1
      LEFT JOIN
      (
        SELECT wifi.*
        FROM
          iot_wifi wifi
          LEFT JOIN iot_streetlight_install_dev_rel install_dev_rel
            ON install_dev_rel.status = ${@com.rz.iot.think.utils.RzIotDBConstParam@DATA_STATUS_OF_NORMAL} AND install_dev_rel.dev_type = ${@com.rz.iot.think.utils.RzIotDBConstParam@STREET_LIGHT_DEV_TYPE_OF_WIFI}
        WHERE
          wifi.status != ${@com.rz.iot.think.utils.RzIotDBConstParam@DEVICE_STATUS_OF_DELETE}
          AND wifi.id = install_dev_rel.dev_id
      ) t2
        ON t1.id = t2.id
    WHERE
      t2.id IS NULL
  </select>
