package com.rz.iot.think.model;

import java.util.HashMap;
import java.util.List;

public class StreetlightSwitch {
    private Integer streetlightId;

    private String streetlightName;

    private String streetlightSn;

    //状态 0：未激活；1：正常运行；2：离线；3：故障； 9：已被删除;
    private Integer streetlightStatus;

    private List<HashMap> ledInfos;


    public Integer getStreetlightId() {
        return streetlightId;
    }

    public void setStreetlightId(Integer streetlightId) {
        this.streetlightId = streetlightId;
    }

    public String getStreetlightName() {
        return streetlightName;
    }

    public void setStreetlightName(String streetlightName) {
        this.streetlightName = streetlightName;
    }

    public String getStreetlightSn() {
        return streetlightSn;
    }

    public void setStreetlightSn(String streetlightSn) {
        this.streetlightSn = streetlightSn;
    }

    public List<HashMap> getLedInfos() {
        return ledInfos;
    }

    public void setLedInfos(List<HashMap> ledInfos) {
        this.ledInfos = ledInfos;
    }

    public Integer getStreetlightStatus() {
        return streetlightStatus;
    }

    public void setStreetlightStatus(Integer streetlightStatus) {
        this.streetlightStatus = streetlightStatus;
    }
}
